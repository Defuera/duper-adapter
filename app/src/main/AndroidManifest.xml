<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="ru.justd.duperadapterdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="ru.justd.duperadapterdemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="ru.justd.duperadapterdemo.samples.ArrayListAdapterShowcase"
            android:label="ArrayListAdapter"
            android:parentActivityName="ru.justd.duperadapterdemo.MainActivity"/>
        <activity
            android:name="ru.justd.duperadapterdemo.samples.MultipleTypesAdapterShowcase"
            android:label="ArrayListAdapter"
            android:parentActivityName="ru.justd.duperadapterdemo.MainActivity"/>

    </application>

</manifest>
